# Auto detect text files and perform LF normalization
* text=auto
*.bin filter=lfs diff=lfs merge=lfs -text
asset filter=lfs diff=lfs merge=lfs -text
*.asset filter=lfs diff=lfs merge=lfs -text
*.resS filter=lfs diff=lfs merge=lfs -text
*.assets.resS filter=lfs diff=lfs merge=lfs -text
sharedassets0.assets.resS filter=lfs diff=lfs merge=lfs -text
MageFighter/Library/PlayerDataCache/Win64/Data/shaderassets0.assets.resS filter=lfs diff=lfs merge=lfs -text
MageFighter/Library/PlayerDataCache/Win64/Data/sharedassets0.assets.resS filter=lfs diff=lfs merge=lfs -text
. filter=lfs diff=lfs merge=lfs -text
all filter=lfs diff=lfs merge=lfs -text
*.assets. filter=lfs diff=lfs merge=lfs -text
